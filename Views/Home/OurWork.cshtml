@{
    ViewData["Title"] = "Our Works";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Our Works | Wennovation Hub</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <style>
        :root {
            --primary-color: #f79f4a;
            --primary-light: rgba(247, 159, 74, 0.1);
            --secondary: #368032;
            --secondary-light: rgba(54, 128, 50, 0.1);
            --light: #f8f9fa;
            --dark: #2a2d34;
            --gray: #6c757d;
            --transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
        }
        
        body {
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
            color: var(--dark);
            overflow-x: hidden;
        }
        
        .hero-section {
            background: linear-gradient(135deg, var(--primary-light) 0%, var(--secondary-light) 100%);
            padding: 5rem 0;
            position: relative;
            overflow: hidden;
        }
        
        .hero-section::before {
            content: "";
            position: absolute;
            top: -50px;
            right: -50px;
            width: 200px;
            height: 200px;
            background-color: var(--primary-light);
            border-radius: 50%;
            z-index: 0;
        }
        
        .hero-section::after {
            content: "";
            position: absolute;
            bottom: -100px;
            left: -100px;
            width: 300px;
            height: 300px;
            background-color: var(--secondary-light);
            border-radius: 50%;
            z-index: 0;
        }
        
        .section-title {
            position: relative;
            display: inline-block;
            margin-bottom: 2rem;
            font-weight: 700;
            color: var(--dark);
        }
        
        .section-title::after {
            content: "";
            position: absolute;
            bottom: -10px;
            left: 0;
            width: 50px;
            height: 4px;
            background-color: var(--primary-color);
            border-radius: 2px;
        }
        
        .partner-card {
            border: none;
            border-radius: 10px;
            overflow: hidden;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);
            transition: var(--transition);
            height: 100%;
            display: flex;
            flex-direction: column;
        }
        
        .partner-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
        }
        
        .partner-card .card-img-top {
            height: 120px;
            object-fit: contain;
            padding: 1.5rem;
            background-color: var(--light);
        }
        
        .partner-card .card-body {
            flex: 1;
            display: flex;
            flex-direction: column;
        }
        
        .partner-card .card-title {
            color: var(--secondary);
            font-weight: 600;
            margin-bottom: 0.75rem;
        }
        
        .partner-card .card-text {
            color: var(--gray);
            flex-grow: 1;
            margin-bottom: 1rem;
        }
        
        .partner-card .partner-meta {
            font-size: 0.85rem;
            color: var(--gray);
            margin-bottom: 1rem;
        }
        
        .partner-card .partner-meta i {
            color: var(--primary-color);
            margin-right: 5px;
        }
        
        .btn-primary-custom {
            background-color: var(--primary-color);
            border-color: var(--primary-color);
            color: white;
            font-weight: 500;
            padding: 0.5rem 1.5rem;
            border-radius: 50px;
            transition: var(--transition);
        }
        
        .btn-primary-custom:hover {
            background-color: #e68e3f;
            border-color: #e68e3f;
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(247, 159, 74, 0.3);
            color: white;
        }
        
        .btn-outline-custom {
            border-color: var(--primary-color);
            color: var(--primary-color);
            font-weight: 500;
            padding: 0.5rem 1.5rem;
            border-radius: 50px;
            transition: var(--transition);
        }
        
        .btn-outline-custom:hover {
            background-color: var(--primary-color);
            color: white;
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(247, 159, 74, 0.3);
        }
        
        .modal-content {
            border: none;
            border-radius: 15px;
            overflow: hidden;
        }
        
        .modal-header {
            background-color: var(--secondary);
            color: white;
            border-bottom: none;
            padding: 1.5rem;
        }
        
        .modal-body {
            padding: 2rem;
        }
        
        .modal-body h5 {
            color: var(--secondary);
            font-weight: 600;
            margin-top: 1.5rem;
        }
        
        .modal-body ul {
            padding-left: 1.5rem;
        }
        
        .modal-body ul li {
            margin-bottom: 0.5rem;
        }
        
        .close {
            color: white;
            opacity: 1;
            text-shadow: none;
        }
        
        .portfolio-section {
            background-color: var(--light);
            padding: 5rem 0;
        }
        
        .portfolio-filter {
            margin-bottom: 2rem;
        }
        
        .filter-btn {
            background: none;
            border: none;
            color: var(--gray);
            font-weight: 500;
            padding: 0.5rem 1.5rem;
            margin: 0 0.5rem;
            border-radius: 50px;
            transition: var(--transition);
        }
        
        .filter-btn.active, .filter-btn:hover {
            background-color: var(--primary-color);
            color: white;
        }
        
        .startup-card {
            border: none;
            border-radius: 10px;
            overflow: hidden;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);
            transition: var(--transition);
            margin-bottom: 2rem;
            height: 100%;
        }
        
        .startup-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
        }
        
        .startup-card .card-img-top {
            height: 180px;
            @* object-fit: cover; *@
        }
        
        .startup-card .card-body {
            padding: 1.5rem;
        }
        
        .startup-card .card-title {
            color: var(--secondary);
            font-weight: 600;
            margin-bottom: 0.75rem;
        }
        
        .startup-card .sector-badge {
            display: inline-block;
            background-color: var(--primary-light);
            color: var(--primary-color);
            padding: 0.25rem 0.75rem;
            border-radius: 50px;
            font-size: 0.75rem;
            font-weight: 600;
            margin-bottom: 0.75rem;
        }
        
        .startup-card .card-text {
            color: var(--gray);
            margin-bottom: 1rem;
        }
        
        .startup-card .quote {
            font-style: italic;
            color: var(--dark);
            position: relative;
            padding: 1rem;
            background-color: var(--light);
            border-radius: 5px;
            margin: 1rem 0;
        }
        
        .startup-card .quote::before {
            content: '"';
            font-size: 3rem;
            color: var(--primary-light);
            position: absolute;
            top: -15px;
            left: 5px;
            line-height: 1;
        }
        
        .nav-pills .nav-link.active {
            background-color: var(--primary-color);
        }
        
        .nav-pills .nav-link {
            color: var(--dark);
        }
        
        .tab-content {
            padding: 2rem 0;
        }
        
        media (max-width: 768px) {
            .hero-section {
                padding: 3rem 0;
            }
            
            .section-title {
                font-size: 1.75rem;
            }
            
            .filter-btn {
                margin: 0.25rem;
                padding: 0.4rem 1rem;
            }
        }
    </style>
</head>
